{
  "name": "vite-plugin-flexible",
  "version": "1.0.1",
  "description": "Vite plugin that automatically injects flexible scripts and converts pixel units to rem units",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "author": "dominicleo",
  "keywords": [
    "vite"
  ],
  "homepage": "https://github.com/dominicleo/vite-plugin-flexible#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/dominicleo/vite-plugin-flexible"
  },
  "bugs": "https://github.com/dominicleo/vite-plugin-flexible/issues",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm run build -- --watch",
    "build": "tsup src/index.ts --dts --format cjs,esm",
    "prepublishOnly": "npm run build",
    "release": "git-ensure -a && bumpp --commit --tag --push",
    "lint": "eslint --ext .ts --ignore-path .gitignore --ignore-pattern \"!**/.*\" .",
    "update:deps": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "postcss-pxtorem": "^6.0.0",
    "vite": "^2.0.5"
  },
  "devDependencies": {
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "bumpp": "^6.0.6",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "git-ensure": "^0.1.0",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "tsup": "^4.6.1",
    "typescript": "^4.2.3"
  }
}
